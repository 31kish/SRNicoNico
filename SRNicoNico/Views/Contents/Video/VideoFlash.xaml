<UserControl x:Class="SRNicoNico.Views.Contents.Video.VideoFlash"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SRNicoNico.Views.Contents.Video"
             xmlns:vm="clr-namespace:SRNicoNico.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="208" d:DesignWidth="401"
             d:DataContext="{d:DesignInstance vm:VideoViewModel}" DataContextChanged="UserControl_DataContextChanged"
			 >
    <Grid>
        <WebBrowser x:Name="browser" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" LoadCompleted="browser_LoadCompleted" />
		
    </Grid>
</UserControl>
